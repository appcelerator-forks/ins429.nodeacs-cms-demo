<% var o = obj.files[0] %>
<div id="ctnr">
  <div class="form-ctnr">
    <form name="update" action="/files/<%= req.params.id %>/update" method="post" enctype="multipart/form-data">
      <div><label>file id:</label><span><%= (o.id) ? o.id : '' %></span></div>
      <div><label>file:</label><input type="file" name="file"></div>
      <div><label>name:</label><input type="text" name="name" value="<%= (o.name) ? o.name : '' %>"/></div>
      <div><label>tags:</label><input type="text" name="tags" placeholder="a,b,c" value="<%= (o.tags) ? o.tags : '' %>"/></div>
      <input class="fr" type="submit" value="Submit"><a href="/files" class="cncl fr">Cancel</a>
      <div class="clr"></div>
    </form>
  </div>
</div>