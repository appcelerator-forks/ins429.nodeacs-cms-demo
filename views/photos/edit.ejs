<% var o = obj.photos[0] %>
<div id="ctnr">
  <div class="form-ctnr">
    <form name="update" action="/photos/<%= req.params.id %>/update" method="post" enctype="multipart/form-data">
      <div><label>photo id:</label><span><%= (o.id) ? o.id : '' %></span></div>
      <div><label>photo:</label><input type="file" name="photo" /></div>
      <div><label>collection id:</label><input type="text" name="collection_id" value="<%= (o.collection_id) ? o.collection_id : '' %>"/></div>
      <div><label>tags:</label><input type="text" name="tags" placeholder="a,b,c" value="<%= (o.tags) ? o.tags : '' %>"/></div>
      <input class="fr" type="submit" value="Submit"><a href="/photos" class="cncl fr">Cancel</a>
      <div class="clr"></div>
    </form>
  </div>
</div>